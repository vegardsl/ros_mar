
<robot name="MAR">

  <material name="blue">
    <color rgba="0 0 .8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="silver">
    <color rgba="0.75 0.75 0.75 1"/>
  </material>

<!-- BASE LINK -->

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.8 0.37 0.005"/>
      </geometry>
      <material name="silver"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.8 0.37 0.005"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="4" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.04564" ixy="0.0" ixz="0.0"
               iyy="0.21334" iyz="0.0" 
               izz="0.25879" />
    </inertial>
  </link>

<!-- REAR COMPARTMENT -->

  <link name="rear_compartment">
    <visual>
      <geometry>
        <box size="0.35 0.37 0.40"/>
      </geometry>
      <material name="silver"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.35 0.37 0.40"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="20" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.49483" ixy="0.0" ixz="0.0"
               iyy="0.47083" iyz="0.0" 
               izz="0.43233" />
    </inertial>
  </link>

  <joint name="base_to_rear_compartment" type="fixed">	
    <parent link="base_link"/>
    <child link="rear_compartment"/>
    <origin xyz="-0.225 0 0.205"/>
  </joint>

<!-- MANIPULATOR ARM DUMMY -->

  <link name="manipulator_arm_dummy">
    <visual>
      <geometry>
        <cylinder length="0.35" radius="0.125"/>
      </geometry>
      <origin xyz="0 0. 0.175"/>
    </visual>

    <collision>
      <geometry>
        <cylinder length="0.35" radius="0.125"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="7" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0988" ixy="0.0" ixz="0.0"
               iyy="0.0988" iyz="0.0" 
               izz="0.00911" />
    </inertial>
  </link>

  <joint name="base_to_manipulator_arm_dummy" type="fixed">	
    <parent link="base_link"/>
    <child link="manipulator_arm_dummy"/>
    <origin xyz="0.21 0 0.005"/>
  </joint>

<!-- DRIVE COMPARTMENT -->

  <link name="drive_compartment">
    <visual>
      <geometry>
        <box size="0.44 0.365 0.055"/>
      </geometry>
      <origin xyz="0 0. -0.0275"/>
      <material name="silver"/>
    </visual>

    <collision>
      <geometry>
        <box size="0.44 0.365 0.055"/>
      </geometry>
      <origin xyz="0 0. -0.0275"/>
    </collision>

    <inertial>
      <mass value="1.2" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01321" ixy="0.0" ixz="0.0"
               iyy="0.01625" iyz="0.0" 
               izz="0.02896" />
    </inertial>
  </link>

  <joint name="base_to_drive_compartment" type="fixed">	
    <parent link="base_link"/>
    <child link="drive_compartment"/>
    <origin xyz="0 0 -0.0025"/>
  </joint>

<!-- WHEEL LINKS -->

  <link name="left_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.05"/>
      </geometry>
      <material name="blue"/>
    </visual>

    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.05"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.816" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.00884" ixy="0.0" ixz="0.0"
               iyy="0.00017" iyz="0.0" 
               izz="0.00884" />
    </inertial>
  </link>

  <link name="right_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.05"/>
      </geometry>
      <material name="blue"/>
    </visual>

    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.05"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.816" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.00884" ixy="0.0" ixz="0.0"
               iyy="0.00017" iyz="0.0" 
               izz="0.00884" />
    </inertial>
  </link>

  <link name="left_back_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.05"/>
      </geometry>
      <material name="blue"/>
    </visual>

    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.05"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.816" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.00884" ixy="0.0" ixz="0.0"
               iyy="0.00017" iyz="0.0" 
               izz="0.00884" />
    </inertial>
  </link>

  <link name="right_back_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.05"/>
      </geometry>
      <material name="blue"/>
    </visual>

    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.055" radius="0.05"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.816" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.00884" ixy="0.0" ixz="0.0"
               iyy="0.00017" iyz="0.0" 
               izz="0.00884" />
    </inertial>
  </link>


<!-- WHEEL JOINTS -->

  <joint name="left_front_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="drive_compartment"/>
    <child link="left_front_wheel"/>
    <origin rpy="0 0 0" xyz="0.18 0.2225 -0.0275"/>
  </joint>

  <joint name="right_front_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="drive_compartment"/>
    <child link="right_front_wheel"/>
    <origin rpy="0 0 0" xyz="0.18 -0.2225 -0.0275"/>
  </joint>

  <joint name="left_back_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="drive_compartment"/>
    <child link="left_back_wheel"/>
    <origin rpy="0 0 0" xyz="-0.18 0.2225 -0.0275"/>
  </joint>

  <joint name="right_back_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="drive_compartment"/>
    <child link="right_back_wheel"/>
    <origin rpy="0 0 0" xyz="-0.18 -0.2225 -0.0275"/>
  </joint>

<!-- CAMERA LINKS -->

  <link name="camera_link">
    <visual>
      <geometry>
        <box size="0.064 0.121 0.0381" />
      </geometry>
      <material name="Blue" />
    </visual>

    <inertial>
      <mass value="0.0001" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
               iyy="0.0001" iyz="0.0" 
               izz="0.0001" />
    </inertial>
  </link>

  <link name="camera_depth_frame">
    <inertial>
        <mass value="0.0001" />
        <origin xyz="0 0 0" />
        <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
                iyy="0.0001" iyz="0.0" 
                izz="0.0001" />
    </inertial>
  </link>

<!-- LASER LINK -->

  <link name="laser">
    <visual>
      <geometry>
        <box size="0.04 0.04 0.07" />
      </geometry>
      <material name="Blue" />
    </visual>
    
    <inertial>
      <mass value="0.0001" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
               iyy="0.0001" iyz="0.0" 
               izz="0.0001" />
    </inertial>
  </link>

<!-- SENSOR JOIÇ¸TS -->

  <joint name="camera_frame_joint" type="fixed">
    <parent link="base_link"/>
    <child link="camera_link"/>
    <origin xyz="0.36 0 0.13"/>
  </joint>

  <joint name="camera_depth_joint" type="fixed">
    <origin xyz="0 0.028 0" rpy="0 0 0" />
    <parent link="camera_link" />
    <child link="camera_depth_frame" />
  </joint>

  <joint name="laser_joint" type="fixed">
    <parent link="base_link"/>
    <child link="laser"/>
    <origin xyz="0.36 0 0.0375"/>
  </joint>

  <gazebo reference="MAR">
    <material>Gazebo/Blue</material>
  </gazebo>

</robot>
